{
    "name": "web-ruslan/laravel-pretty-pagination",
    "description": "Laravel SEO friendly pagination routing.",
    "keywords": [
        "web-ruslan",
        "laravel-pretty-pagination",
        "SEO",
        "laravel",
        "pagination",
        "routing"
    ],
    "homepage": "https://github.com/web-ruslan/laravel-pretty-pagination",
    "license": "MIT",
    "authors": [
        {
            "name": "Ruslan Rieznikov",
            "email": "webruslan@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3|^8",
        "illuminate/contracts": "5.8.*|^6|^7|^8|^9",
        "illuminate/routing": "5.8.*|^6|^7|^8|^9",
        "illuminate/translation": "5.8.*|^6|^7|^8|^9",
        "illuminate/pagination": "5.8.*|^6|^7|^8|^9",
        "ext-json": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.3|^10.0"
    },
    "autoload": {
        "psr-4": {
            "WebRuslan\\PaginateRoute\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebRuslan\\PaginateRoute\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "WebRuslan\\PaginateRoute\\PaginateRouteServiceProvider"
            ],
            "aliases": {
                "PaginateRoute": "WebRuslan\\PaginateRoute\\PaginateRouteFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
